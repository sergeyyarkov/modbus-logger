<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="modbus-logger" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/modbus-logger" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="lib/libmodbus_win32" />
				</Compiler>
				<Linker>
					<Add library="lib/libmodbus_win32/modbus.lib" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/modbus-logger" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/Release" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="lib/libmodbus_win32" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="lib/libmodbus_win32/modbus.lib" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="bin/Release/settings.conf" />
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
